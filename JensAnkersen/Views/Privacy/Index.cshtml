@model ControllerName
@{
    ViewData["Title"] = "Privatlivspolitik";

    string controllerstring = "";
    if (!String.IsNullOrWhiteSpace(Model.ControllerString))
    {
        controllerstring = Model.ControllerString;
    }
}

<div class="text-center">
    <h1 class="display-3">@ViewData["Title"]</h1>
</div>
<body>
    <div class="page_subheader text-center">
        På <a href="https://jensankersen.dk">jensankersen.dk</a> indsamles der ingen personlige oplysninger ved navigation rundt på siden. <br /><br />
        
        Der indsamles kun diagnosticerings data, og disse vil kun blive brugt for at forsøge at rette potentielle fejl der måtte opstå på hjemmesiden. <br /><br />
        
        <u>Ingen</u> af disse diagnosticeringsdata kan spores tilbage til en individuel besøgende på hjemmesiden.

    </div>
</body>
<div class="text-center bottom-nav">
    <a class="thumbnail" asp-area="" asp-controller="@controllerstring" asp-action="Index" asp-fragment="BackToTop">Tilbage til toppen</a> |
    <a class="thumbnail" asp-area="" asp-controller="Home" asp-action="Index" asp-fragment="BackToTop">Tilbage til forsiden</a>
</div>